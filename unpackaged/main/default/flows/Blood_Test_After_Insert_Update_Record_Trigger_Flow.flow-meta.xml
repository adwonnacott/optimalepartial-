    <decisions>
        <name>Check_Blood_Test_Fields</name>
        <label>Check Blood Test Fields</label>
        <locationX>842</locationX>
        <locationY>384</locationY>
        <defaultConnectorLabel>Default</defaultConnectorLabel>
        <rules>
            <name>Veryhighlevels</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Testosterone__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>35.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Template</targetReference>
            </connector>
            <label>Very high levels</label>
        </rules>
        <rules>
            <name>Normallevels</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Testosterone__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>15.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Free_Testosterone__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>0.225</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Copy_1_of_Get_Template</targetReference>
            </connector>
            <label>Normal levels</label>
        </rules>
        <rules>
            <name>Verylow</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Testosterone__c</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>5.4</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Copy_2_of_Get_Template</targetReference>
            </connector>
            <label>Very low</label>
        </rules>
        <rules>
            <name>LowTT</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Testosterone__c</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>15.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Free_Testosterone__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>0.225</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Copy_3_of_Get_Template</targetReference>
            </connector>
            <label>Low TT</label>
        </rules>
        <rules>
            <name>LowFT</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Testosterone__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>15.0</numberValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Free_Testosterone__c</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>0.225</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Copy_4_of_Get_Template</targetReference>
            </connector>
            <label>Low FT</label>
        </rules>
        <rules>
            <name>Low_both</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Testosterone__c</leftValueReference>
                <operator>LessThanOrEqualTo</operator>

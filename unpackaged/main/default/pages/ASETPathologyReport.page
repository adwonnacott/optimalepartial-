<apex:page standardController="Blood_Test__c" extensions="ASETPathologyReportController" renderAs="pdf" applyBodyTag="false">
    <head>
        <style type="text/css" media="print">
            @page {
                margin-top: 320px;
                padding: 0 14px 0 0;
                margin-bottom: 120px;
                size:A4 portrait;
            
                @top-center {
                    content: element(header);
                }

                @bottom-center {
                    content: element(footer);
                }

            }

            * {
                font-family:"Calibri", sans-serif;
            }

            .centered {
            	text-align: center;
            }

            div.header {
                /* height: 1000px;
                margin-top:20px;
                margin-bottom: 120px;
                padding-bottom: 120px; */
                position: running(header);
                width: 98%;
                padding: 10px;
                font-size: 22px;
                font-family:"Calibri", sans-serif;
            }

            div.content {
                margin-top: 120px;
                width: 100%;
            }

            div.footer {
                margin-top:24px;
                position: running(footer);
                font-size: 14px;
            }

            .pagenumber:before {
                content: counter(page);
            }

            .pagecount:before {
            	content: counter(pages);
            }

            table tbody td {
                padding: 0;
                margin: auto;
                border: none;   
            }

            table {
                border-collapse: collapse;
                font-family:"Calibri", sans-serif;
                font-size: 14px;
            }     
         
            table tr .title {
                padding: 12px 0;
                font-weight: bold;
            }  

            table thead th {
                border-left: none;
                border-right: none;
                border-top: 1px solid #0d80d3;
                border-bottom: 1px solid #0d80d3;
                padding-left: 0;
                padding-right: 0;
                color: #0d80d3;
                width: 200px;
                padding-top: 10px;
                padding-bottom: 10px;
            }

            table thead th.invis-theader{
                border-left: none;
                border-right: none;
                border-top: 1px solid transparent;
                border-bottom: 1px solid transparent;
                padding-left: 0;
                padding-right: 0;
                color: #0d80d3;
                width: 200px;
                padding-top: 0px !important;
                padding-bottom: 0px !important;
            }

            .color-red {
                color: red;
            }

            .color-blue {
                color: #0d80d3;
            }
            
            .header-table td {
                width: 782px; 
                vertical-align: baseline;
            }

            .contact-label {
                color: #0d80d3;
                width: 600px;
                text-align: end; 
            }

            img {
                margin-bottom: 20px;
            }

            .optimale--details tr td {
                width: 238px!important;
                padding-right: 20px;
            }

            .optimale--details tr td:nth-child(2){
                text-align: left;
            }

            .optimale--details .hide {
                display: none;
            }

        </style>
    </head>
    <body>
        <div class="header" style="padding-bottom: 240px;page-break-after:always;font-size: 18px;">
            <div class="header-content">
                <div class="logo">
                    <apex:image url="{!$Resource.OptimaleLogo}" width="240" height="60"/>
                </div>
                <div class="col">
                    <table>
                        <tbody class="header-table">
                            <tr>
                                <td>
                                    <div class="address">
                                        71-75 Shelton Street <br />
                                        Covent Garden <br />
                                        London <br />
                                        WC2H 9JQ <br />
                                        0207 164 6911 <br />
                                        bloods@optimale.co.uk <br />
                                    </div>
                                </td>
                                <td class="contac-details">
                                    <apex:outputText >
                                        <span class="contact-label">Surname : </span> <apex:outputText value="{!contact.LastName}"/>
                                        <br />
                                    </apex:outputText>
                                    <apex:outputText >
                                        <span class="contact-label">Forename : </span> <apex:outputText value="{!contact.FirstName}"/>
                                        <br />
                                    </apex:outputText>
                                    <apex:outputText >
                                        <span class="contact-label">Date of birth : </span> 
                                        <apex:outputText value="{0,date,dd/MM/yyyy}">
                                            <apex:param value="{!contact.Date_of_Birth__c}"/>
                                        </apex:outputText>
                                        <br />
                                    </apex:outputText>
                                    <apex:outputText >
                                        <span class="contact-label">Patient ID : </span> <apex:outputText value="{!contact.Patient_ID__c}"/>
                                        <br />
                                    </apex:outputText>
                                    <apex:outputText >
                                        <span class="contact-label">Blood Test ID : </span> <apex:outputText value="{!btRecord.Name}"/>
                                        <br />
                                    </apex:outputText>
                                    <apex:outputText >
                                        <span class="contact-label">Sex: </span> <apex:outputText value="{!contact.Gender__c}"/>
                                        <br />
                                    </apex:outputText>
                                    <apex:outputText >
                                        <span class="contact-label">Lab No : </span> <apex:outputText value="{!btRecord.Labs_Test_ID__c}"/> 
                                        <br />
                                    </apex:outputText>
                                    <apex:outputText >
                                        <span class="contact-label">Sample Date : </span>
                                        <apex:outputText value="{!sampleDateStr}"/>
                                        <!--<apex:outputText value="{0,date,dd/MM/yyyy HH:mm:ss}">
                                            					
                                            <apex:param value="{!btRecord.Sample_Date_Time__c}"/>
                                        </apex:outputText>-->
                                        <br />
                                    </apex:outputText>
                                    <apex:outputText >
                                        <span class="contact-label">Sample Type : </span> <apex:outputText value="{!btRecord.Collection_Method__c}"/> 
                                        <br />
                                    </apex:outputText>
                                    <apex:outputText >
                                        <span class="contact-label">Age : </span> <apex:outputText value="{!contact.account.Age__pc}"/> 
                                        <br />
                                    </apex:outputText>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="table-header-wrapper" style="padding-top: 20px;">
                <table style="padding-top: 20px; margin-left: -2px; table-layout: fixed; width: 100%;">
                    <thead class="tableHeader">
                        <tr>
                            <th style="width: 178px;">Test</th>
                            <th style="width: 158px;">Patient Result</th>
                            <th style="width: 168px;">Normal Range</th>
                            <th style="width: 168px;">Units</th>
                            <!-- <th style="width: 138px;">Units</th> -->
                        </tr>
                    </thead>

                </table>
            </div>
        </div>
        <div class="footer" style="page-break-after:always;font-size: 14px;">
            <div style="background:#0d80d3;"></div>
            <br/>
            <div class="page">
                <div class="subfooter centered">Page <span class="pagenumber"/> of <span class="pagecount"/></div>
            </div>
            <br/>
            <!--<div class="centered" style="color: #0d80d3;font-size:12px">
                * Referred test, contact lab for details. **Awaiting - application submitted to UKAS, internally verified by clinical consultants V=Venous, F=Fingerprick 
                <br/> NB. In house Allergy Testing on Phadia 250 was introduced on 1st March 2022.
            </div>-->
        </div>
        <div class="content">
            <table style="padding-bottom: 24px; table-layout: fixed; " border="0">
                <thead class="tableHeader" style="visibility: hidden;">
                    <tr style="line-height: 0px !important; height: 0px;">
                        <th class="invis-theader" style="width: 178px; height: 0px !important; line-height: 0px !important;" height="0px">Test</th>
                        <th class="invis-theader" style="width: 158px; height: 0px !important; line-height: 0px !important;" height="0px">Patient Result</th>
                        <th class="invis-theader" style="width: 168px; height: 0px !important; line-height: 0px !important;" height="0px">Normal Range</th>
                        <th class="invis-theader" style="width: 168px; height: 0px !important; line-height: 0px !important;" height="0px">Units</th>
                    </tr>
                </thead>
                <tbody class="optimale--details">
                    <tr class="{!IF( NOT( ISBLANK( btRecord.Haemoglobin__c ) ) || NOT( ISBLANK( btRecord.Red_Blood_Cells__c ) ) || NOT( ISBLANK( btRecord.Haematocrit_Adj__c ) ) || NOT( ISBLANK( btRecord.Haematocrit__c ) ) || NOT( ISBLANK( btRecord.Mean_Cell_Volume__c ) ) || NOT( ISBLANK( btRecord.Red_Cell_Distribution__c ) ) || NOT( ISBLANK( btRecord.Mean_Cell_Haemoglobin__c ) ) || NOT( ISBLANK( btRecord.MCHC__c ) ) || NOT( ISBLANK( btRecord.Platelets__c ) ) || NOT( ISBLANK( btRecord.MPV__c ) ) || NOT( ISBLANK( btRecord.White_Blood_Cells__c ) ) || NOT( ISBLANK( btRecord.Neutrophils__c ) ) || NOT( ISBLANK( btRecord.Lymphocytes__c ) ) || NOT( ISBLANK( btRecord.Monocytes__c ) ) || NOT( ISBLANK( btRecord.Eosinophils__c ) ) || NOT( ISBLANK( btRecord.Basophils__c ) ) || NOT( ISBLANK( btRecord.Cortisol__c ) ) || NOT( ISBLANK( btRecord.Magnesium__c ) ), '', 'hide')}">
                        <td class="title">Full Blood Count</td>
                    </tr>
                    <tr class="{!IF( 
                                NOT( ISBLANK( btRecord.Haemoglobin__c ) ), 
                                    IF(NOT(ISBLANK(btRecord.Haemoglobin_Min__c	)) || NOT(ISBLANK(btRecord.Haemoglobin_Max__c	)),
                                         IF(btRecord.Haemoglobin__c < VALUE(btRecord.Haemoglobin_Min__c) || btRecord.Haemoglobin__c >  VALUE(btRecord.Haemoglobin_Max__c), 
                                            'color-red'	, '' ) , 
                                         '' ) ,
                                     'hide'
                                )}">
                        <td> Haemoglobin</td>
                        <td style="width: 100px;">
                            <apex:outputText value="{0,number,#,##0.000}">
                                <apex:param value="{!IF(NOT(ISBLANK(btRecord.Haemoglobin__c	)), btRecord.Haemoglobin__c, 0 )}" />
                            </apex:outPutText>
                        </td>
                        <td >
                            <span class="status">
                                <apex:outputText rendered="{!IF( IF(NOT(ISBLANK(btRecord.Haemoglobin__c)), btRecord.Haemoglobin__c, 0 ) < IF(NOT(ISBLANK(btRecord.Haemoglobin_Min__c)) , VALUE(btRecord.Haemoglobin_Min__c), 0), true, false)}">
                                    <apex:outputText value="{!low}"/>
                                    &nbsp;
                                </apex:outputText>
                                <apex:outputText rendered="{!IF(  IF(NOT(ISBLANK(btRecord.Haemoglobin__c)), btRecord.Haemoglobin__c, 0 ) > IF(  NOT( ISBLANK( btRecord.Haemoglobin_Max__c ) ) , VALUE(btRecord.Haemoglobin_Max__c), 0) , true, false)}">
                                    <apex:outputText value="{!high}" />
                                    &nbsp;
                                </apex:outputText>
                           </span>
                           <span class="normal-range">
                                <apex:outputText value="{!btRecord.Haemoglobin_Min__c}"/> - <apex:outputText value="{!btRecord.Haemoglobin_Max__c}"/>
                           </span>
                        </td>
                        <td>
                            <apex:outputText value="{!btRecord.Haemoglobin_Units__c}"/>
                        </td>
                    </tr>
                    <tr class="{!IF( 
                                NOT( ISBLANK( btRecord.Red_Blood_Cells__c ) ), 
                                    IF(NOT(ISBLANK(btRecord.Red_Blood_Cells_Min__c	)) || NOT(ISBLANK(btRecord.Red_Blood_Cells_Max__c	)),
                                         IF(btRecord.Red_Blood_Cells__c < VALUE(btRecord.Red_Blood_Cells_Min__c) || btRecord.Red_Blood_Cells__c >  VALUE(btRecord.Red_Blood_Cells_Max__c), 
                                            'color-red'	, '' ) , 
                                         '' ) ,
                                     'hide'
                                )}">
                        <td>Red Blood Cells</td>
                        <td>
                            <apex:outputText value="{0,number,#,##0.000}">
                                <apex:param value="{!IF(NOT(ISBLANK(btRecord.Red_Blood_Cells__c	)), btRecord.Red_Blood_Cells__c, 0 )}" />
                            </apex:outPutText>
                        </td>
                        <td>
                            <span class="status">
                                <apex:outputText rendered="{!IF( IF(NOT(ISBLANK(btRecord.Red_Blood_Cells__c)), btRecord.Red_Blood_Cells__c, 0 ) < IF(NOT(ISBLANK(btRecord.Red_Blood_Cells_Min__c)) , VALUE(btRecord.Red_Blood_Cells_Min__c), 0), true, false)}">
                                    <apex:outputText value="{!low}"/>
                                    &nbsp;
                                </apex:outputText>
                                <apex:outputText rendered="{!IF(  IF(NOT(ISBLANK(btRecord.Red_Blood_Cells__c)), btRecord.Red_Blood_Cells__c, 0 ) > IF(  NOT( ISBLANK( btRecord.Red_Blood_Cells_Max__c ) ) , VALUE(btRecord.Red_Blood_Cells_Max__c), 0) , true, false)}">
                                    <apex:outputText value="{!high}" />
                                    &nbsp;
                                </apex:outputText>
                           </span>
                           <span class="normal-range">
                                <apex:outputText value="{!btRecord.Red_Blood_Cells_Min__c}"/> - <apex:outputText value="{!btRecord.Red_Blood_Cells_Max__c}"/>
                           </span>
                        </td>
                        <td>
                            <apex:outputText value="{!btRecord.Red_Blood_Cells_Units__c}"/>
                        </td>
                    </tr>
                    <tr class="{!IF( 
                        NOT( ISBLANK( btRecord.Haematocrit_Adj__c ) ), 
                            IF(NOT(ISBLANK(btRecord.Haematocrit_Min_Adj__c	)) || NOT(ISBLANK(btRecord.Haematocrit_Max_Adj__c	)),
                                 IF(btRecord.Haematocrit_Adj__c < VALUE(btRecord.Haematocrit_Min_Adj__c) || btRecord.Haematocrit_Adj__c >  VALUE(btRecord.Haematocrit_Max_Adj__c), 
                                    'color-red'	, '' ) , 
                                 '' ) ,
                             'hide'
                        )}">
                            <td>Haematocrit </td>
                            <td>
                                <apex:outputText value="{0,number,#,##0.000}">
                                    <apex:param value="{!IF(NOT(ISBLANK(btRecord.Haematocrit_Adj__c	)), btRecord.Haematocrit_Adj__c, 0 )}" />
                                </apex:outPutText>
                            </td>
                            <td>
                                <span class="status">
                                    <apex:outputText rendered="{!IF( IF(NOT(ISBLANK(btRecord.Haematocrit_Adj__c)), btRecord.Haematocrit_Adj__c, 0 ) < IF(NOT(ISBLANK(btRecord.Haematocrit_Min_Adj__c)) , VALUE(btRecord.Haematocrit_Min_Adj__c), 0), true, false)}">
                                        <apex:outputText value="{!low}"/>
                                        &nbsp;
                                    </apex:outputText>
                                    <apex:outputText rendered="{!IF(  IF(NOT(ISBLANK(btRecord.Haematocrit_Adj__c)), btRecord.Haematocrit_Adj__c, 0 ) > IF(  NOT( ISBLANK( btRecord.Haematocrit_Max_Adj__c ) ) , VALUE(btRecord.Haematocrit_Max_Adj__c), 0) , true, false)}">
                                        <apex:outputText value="{!high}" />
                                        &nbsp;
                                    </apex:outputText>
                                </span>
                                <span class="normal-range">
                                        <apex:outputText value="{!btRecord.Haematocrit_Min_Adj__c}"/> - &nbsp;<apex:outputText value="{!btRecord.Haematocrit_Max_Adj__c}"/>
                                </span>
                            </td>
                            <td>
                                <apex:outputText value="{!btRecord.Haematocrit_Units_Adj__c}"/>
                            </td>
                        </tr>
                    <tr class="{!IF( 
                                NOT( ISBLANK( btRecord.Haematocrit__c ) ) && ISBLANK( btRecord.Haematocrit_Adj__c ), 
                                    IF(NOT(ISBLANK(btRecord.Haematocrit_Min__c	)) || NOT(ISBLANK(btRecord.Haematocrit_Max__c	)),
                                         IF(btRecord.Haematocrit__c < VALUE(btRecord.Haematocrit_Min__c) || btRecord.Haematocrit__c >  VALUE(btRecord.Haematocrit_Max__c), 
                                            'color-red'	, '' ) , 
                                         '' ) ,
                                     'hide'
                                )}">
                        <td>Haematocrit</td>
                        <td>
                            <apex:outputText value="{0,number,#,##0.000}">
                                <apex:param value="{!IF(NOT(ISBLANK(btRecord.Haematocrit__c	)), btRecord.Haematocrit__c, 0 )}" />
                            </apex:outPutText>
                        </td>
                        <td>
                            <span class="status">
                                <apex:outputText rendered="{!IF( IF(NOT(ISBLANK(btRecord.Haematocrit__c)), btRecord.Haematocrit__c, 0 ) < IF(NOT(ISBLANK(btRecord.Haematocrit_Min__c)) , VALUE(btRecord.Haematocrit_Min__c), 0), true, false)}">
                                    <apex:outputText value="{!low}"/>
                                    &nbsp;
                                </apex:outputText>
                                <apex:outputText rendered="{!IF(  IF(NOT(ISBLANK(btRecord.Haematocrit__c)), btRecord.Haematocrit__c, 0 ) > IF(  NOT( ISBLANK( btRecord.Haematocrit_Max__c ) ) , VALUE(btRecord.Haematocrit_Max__c), 0) , true, false)}">
                                    <apex:outputText value="{!high}" />
                                    &nbsp;
                                </apex:outputText>
                           </span>
                           <span class="normal-range">
                                <apex:outputText value="{!btRecord.Haematocrit_Min__c}"/> - &nbsp;<apex:outputText value="{!btRecord.Haematocrit_Max__c}"/>
                           </span>
                        </td>
                        <td>
                            <apex:outputText value="{!btRecord.Haematocrit_Units__c}"/>
                        </td>
                    </tr>
                    <tr class="{!IF( 
                                NOT( ISBLANK( btRecord.Mean_Cell_Volume__c ) ), 
                                    IF(NOT(ISBLANK(btRecord.Mean_Cell_Volume_Min__c	)) || NOT(ISBLANK(btRecord.Mean_Cell_Volume_Max__c	)),
                                         IF(btRecord.Mean_Cell_Volume__c < VALUE(btRecord.Mean_Cell_Volume_Min__c) || btRecord.Mean_Cell_Volume__c >  VALUE(btRecord.Mean_Cell_Volume_Max__c), 
                                            'color-red'	, '' ) , 
                                         '' ) ,
                                     'hide'
                                )}">
                        <td>Mean Cell Volume</td>
                        <td>
                            <apex:outputText value="{0,number,#,##0.000}">
                                <apex:param value="{!IF(NOT(ISBLANK(btRecord.Mean_Cell_Volume__c	)), btRecord.Mean_Cell_Volume__c, 0 )}" />
                            </apex:outPutText>
                        </td>
                        <td>
                            <span class="status">
                                <apex:outputText rendered="{!IF( IF(NOT(ISBLANK(btRecord.Mean_Cell_Volume__c)), btRecord.Mean_Cell_Volume__c, 0 ) < IF(NOT(ISBLANK(btRecord.Mean_Cell_Volume_Min__c)) , VALUE(btRecord.Mean_Cell_Volume_Min__c), 0), true, false)}">
                                    <apex:outputText value="{!low}"/>
                                    &nbsp;
                                </apex:outputText>
                                <apex:outputText rendered="{!IF(  IF(NOT(ISBLANK(btRecord.Mean_Cell_Volume__c)), btRecord.Mean_Cell_Volume__c, 0 ) > IF(  NOT( ISBLANK( btRecord.Mean_Cell_Volume_Max__c ) ) , VALUE(btRecord.Mean_Cell_Volume_Max__c), 0) , true, false)}">
                                    <apex:outputText value="{!high}" />
                                    &nbsp;
                                </apex:outputText>
                           </span>
                           <span class="normal-range">
                                <apex:outputText value="{!btRecord.Mean_Cell_Volume_Min__c}"/> -&nbsp;<apex:outputText value="{!btRecord.Mean_Cell_Volume_Max__c}"/>
                           </span>
                        </td>
                        <td>
                            <apex:outputText value="{!btRecord.Mean_Cell_Volume_Units__c}"/>
                        </td>
                        <td>
                            &nbsp;	
                        </td>
                    </tr>
                    <tr class="{!IF( 
                                NOT( ISBLANK( btRecord.Red_Cell_Distribution__c ) ), 
                                    IF(NOT(ISBLANK(btRecord.Red_Cell_Distribution_Min__c	)) || NOT(ISBLANK(btRecord.Red_Cell_Distribution_Max__c	)),
                                         IF(btRecord.Red_Cell_Distribution__c < VALUE(btRecord.Red_Cell_Distribution_Min__c) || btRecord.Red_Cell_Distribution__c >  VALUE(btRecord.Red_Cell_Distribution_Max__c), 
                                            'color-red'	, '' ) , 
                                         '' ) ,
                                     'hide'
                                )}">
                        <td>Red Cell Distribution</td>
                        <td>
                            <apex:outputText value="{0,number,#,##0.000}">
                                <apex:param value="{!IF(NOT(ISBLANK(btRecord.Red_Cell_Distribution__c	)), btRecord.Red_Cell_Distribution__c, 0 )}" />
                            </apex:outPutText>
                        </td>
                        <td>
                            <span class="status">
                                <apex:outputText rendered="{!IF( IF(NOT(ISBLANK(btRecord.Red_Cell_Distribution__c)), btRecord.Red_Cell_Distribution__c, 0 ) < IF(NOT(ISBLANK(btRecord.Red_Cell_Distribution_Min__c)) , VALUE(btRecord.Red_Cell_Distribution_Min__c), 0), true, false)}">
                                    <apex:outputText value="{!low}"/>
                                    &nbsp;
                                </apex:outputText>
                                <apex:outputText rendered="{!IF(  IF(NOT(ISBLANK(btRecord.Red_Cell_Distribution__c)), btRecord.Red_Cell_Distribution__c, 0 ) > IF(  NOT( ISBLANK( btRecord.Red_Cell_Distribution_Max__c ) ) , VALUE(btRecord.Red_Cell_Distribution_Max__c), 0) , true, false)}">
                                    <apex:outputText value="{!high}" />
                                    &nbsp;
                                </apex:outputText>
                           </span>
                           <span class="normal-range">
                                <apex:outputText value="{!btRecord.Red_Cell_Distribution_Min__c}"/> - <apex:outputText value="{!btRecord.Red_Cell_Distribution_Max__c}"/>
                           </span>
                        </td>
                        <td>
                            <apex:outputText value="{!btRecord.Red_Cell_Distribution_Units__c}"/>
                        </td>
                    </tr>
                    <tr class="{!IF( 
                                NOT( ISBLANK( btRecord.Mean_Cell_Haemoglobin__c ) ), 
                                    IF(NOT(ISBLANK(btRecord.Mean_Cell_Haemoglobin_Min__c	)) || NOT(ISBLANK(btRecord.Mean_Cell_Haemoglobin_Max__c	)),
                                         IF(btRecord.Mean_Cell_Haemoglobin__c < VALUE(btRecord.Mean_Cell_Haemoglobin_Min__c) || btRecord.Mean_Cell_Haemoglobin__c >  VALUE(btRecord.Mean_Cell_Haemoglobin_Max__c), 
                                            'color-red'	, '' ) , 
                                         '' ) ,
                                     'hide'
                                )}">
                        <td>Mean Cell Haemoglobin</td>
                        <td>
                            <apex:outputText value="{0,number,#,##0.000}">
                                <apex:param value="{!IF(NOT(ISBLANK(btRecord.Mean_Cell_Haemoglobin__c	)), btRecord.Mean_Cell_Haemoglobin__c, 0 )}" />
                            </apex:outPutText>
                        </td>
                        <td>
                            <span class="status">
                                <apex:outputText rendered="{!IF( IF(NOT(ISBLANK(btRecord.Mean_Cell_Haemoglobin__c)), btRecord.Mean_Cell_Haemoglobin__c, 0 ) < IF(NOT(ISBLANK(btRecord.Mean_Cell_Haemoglobin_Min__c)) , VALUE(btRecord.Mean_Cell_Haemoglobin_Min__c), 0), true, false)}">
                                    <apex:outputText value="{!low}"/>
                                    &nbsp;
                                </apex:outputText>
                                <apex:outputText rendered="{!IF(  IF(NOT(ISBLANK(btRecord.Mean_Cell_Haemoglobin__c)), btRecord.Mean_Cell_Haemoglobin__c, 0 ) > IF(  NOT( ISBLANK( btRecord.Mean_Cell_Haemoglobin_Max__c ) ) , VALUE(btRecord.Mean_Cell_Haemoglobin_Max__c), 0) , true, false)}">
                                    <apex:outputText value="{!high}" />
                                    &nbsp;
                                </apex:outputText>
                           </span>
                           <span class="normal-range">
                                <apex:outputText value="{!btRecord.Mean_Cell_Haemoglobin_Min__c}"/> - &nbsp;<apex:outputText value="{!btRecord.Mean_Cell_Haemoglobin_Max__c}"/>
                           </span>
                        </td>
                        <td>
                            <apex:outputText value="{!btRecord.Mean_Cell_Haemoglobin_Units__c}"/>
                        </td>
                    </tr>
                    <tr class="{!IF( 
                                NOT( ISBLANK( btRecord.MCHC__c ) ), 
                                    IF(NOT(ISBLANK(btRecord.MCHC_Min__c	)) || NOT(ISBLANK(btRecord.MCHC_Max__c	)),
                                         IF(btRecord.MCHC__c < VALUE(btRecord.MCHC_Min__c) || btRecord.MCHC__c >  VALUE(btRecord.MCHC_Max__c), 
                                            'color-red'	, '' ) , 
                                         '' ) ,
                                     'hide'
                                )}">

    <fullName>Low_Testosterone_Flag__c</fullName>
    <description>Red flag if testosterone results are low</description>
    <externalId>false</externalId>
    <formula>IF (OR (Free_Testosterone__c &lt;= 0.225,  Testosterone__c &lt;= 15.0),


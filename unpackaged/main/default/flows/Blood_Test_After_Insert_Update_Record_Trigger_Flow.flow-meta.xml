    <decisions>
        <name>New_or_Current_Patient</name>
        <label>New or Current/Held Patient</label>
        <locationX>1370</locationX>
        <locationY>276</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>New_Patient</name>
            <conditionLogic>(1 AND 2 AND 3 AND 6 AND 7) AND (4 OR 5)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Contact__r.Patient_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Current Patient</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Contact__r.Patient_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>On Hold</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Contact__r.Patient_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Lost</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Test_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>OPT013-V</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Test_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>OPT013-FP</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Testosterone__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Free_Testosterone__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Check_Blood_Test_Fields</targetReference>
            </connector>
            <label>New Patient</label>
        </rules>
    </decisions>
